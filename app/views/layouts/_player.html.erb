<div class="player-wrapper">
  <div class="inner">
    <div class="img-container col-md-4">
      <img id='artist-photo' src='' alt='Artist Image'>
    </div>
    <div class="visual col-md-6">
      <div class="artist-data mini">
        <h2 class="artist-name"></h2>
        <h2 class="artist-song"></h2>
        <h2 class="artist-album"></h2>
      </div>
      <div class="artist-data slide">
        <div class="inner">
          <h2 class="artist-name"></h2>
          <h2 class="artist-song"></h2>
          <h2 class="artist-album"></h2>
        </div>
      </div>
      <canvas id="canvas" class="canvas"></canvas>
    </div>
    <div class="artist-playlist col-xs-2">
      <h2>Featured Artists:</h2>
      <ul class="list">
        <li class="artist-track">Deftones</li>
        <li class="artist-track">Bjork</li>
        <li class="artist-track">Nine Inch Nails</li>
        <li class="artist-track">Alexisonfire</li>
        <li class="artist-track">Lupe Fiasco</li>
        <li class="artist-track">Mice Parade</li>
        <li class="artist-track">TIAAN</li>
      </ul>
    </div>
  </div>
</div>
<div class="clearfix"></div>
<div class="buttons-wrapper">
  <div class="inner">
    <div class="col-xs-6 col-md-4"></div>
    <div class="col-xs-6 col-md-6">
      <div class="css-change">
        <button class="style1"></button>
        <button class="style2"></button>
      </div>
    </div>
    <div class="col-xs-6 col-md-4"></div>
  </div>
</div>
<div class="clearfix"></div>
<div class="info">
  <div class="bio col-xs-12 col-md-8">
    <p id="artist-bio"></p>
  </div>
  <div class="related col-xs-6 col-md-4"></div>
</div>
<audio id="main-player" controls preload="auto">
</audio>
